<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Loginpanel_client">
   <children>
      <Button layoutX="53.0" layoutY="403.0" mnemonicParsing="false" onAction="#zarejestruj" prefHeight="45.0" prefWidth="175.0" text="ZAŁÓŻ KONTO">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="383.0" layoutY="403.0" mnemonicParsing="false" onAction="#zaloguj" prefHeight="45.0" prefWidth="175.0" text="ZALOGUJ SIĘ">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="69.0" layoutY="65.0" text="Podaj swój identyfikator">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="login" layoutX="69.0" layoutY="92.0" prefHeight="30.0" prefWidth="300.0" />
      <Label layoutX="69.0" layoutY="168.0" text="Podaj swoje hasło">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="69.0" layoutY="195.0" prefHeight="30.0" prefWidth="300.0" />

      <Label layoutX="69.0" layoutY="259.0" text="Na kogo chcesz się zalogować?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="pracownik" layoutX="69.0" layoutY="323.0" mnemonicParsing="false" text="Pracownik" toggleGroup="$wybor_login">
         <font>
            <Font size="15.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="wybor_login" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="klient" layoutX="69.0" layoutY="297.0" mnemonicParsing="false" selected="true" text="Klient" toggleGroup="$wybor_login">
         <font>
            <Font size="15.0" />
         </font>
      </RadioButton>
      <Label layoutX="216.0" layoutY="14.0" text="PANEL LOGOWANIA">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
